#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55a4032ae400 .scope module, "main" "main" 2 1;
 .timescale 0 0;
v0x55a4032cfde0_0 .var "num1", 0 3;
v0x55a4032cff10_0 .var "num2", 0 3;
v0x55a4032d0020_0 .net "res", 0 7, L_0x55a4032d0160;  1 drivers
v0x55a4032d00c0_0 .net "validation", 0 1, L_0x55a4032d0e90;  1 drivers
S_0x55a4032ae580 .scope task, "dispRes" "dispRes" 2 39, 2 39 0, S_0x55a4032ae400;
 .timescale 0 0;
TD_main.dispRes ;
    %vpi_call 2 41 "$display", "Trying operation %d * %d...", v0x55a4032cfde0_0, v0x55a4032cff10_0 {0 0 0};
    %load/vec4 v0x55a4032d00c0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 44 "$display", "Valid input!" {0 0 0};
    %vpi_call 2 45 "$display", "%d * %d = %h", v0x55a4032cfde0_0, v0x55a4032cff10_0, v0x55a4032d0020_0 {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 49 "$display", "Error: Invalid input detected" {0 0 0};
    %load/vec4 v0x55a4032d00c0_0;
    %parti/s 1, 1, 2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %vpi_call 2 52 "$display", "First multiplier error: %d is not a valid one digit BCD number", v0x55a4032cfde0_0 {0 0 0};
T_0.2 ;
    %load/vec4 v0x55a4032d00c0_0;
    %parti/s 1, 0, 2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %vpi_call 2 56 "$display", "Second multiplier error: %d is not a valid one digit BCD number", v0x55a4032cff10_0 {0 0 0};
T_0.4 ;
T_0.1 ;
    %end;
S_0x55a4032ae700 .scope module, "dut" "BCDMultiplier" 2 6, 3 8 0, S_0x55a4032ae400;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "BCDNum1"
    .port_info 1 /INPUT 4 "BCDNum2"
    .port_info 2 /OUTPUT 8 "BCDRes"
    .port_info 3 /OUTPUT 2 "validation"
v0x55a4032cf790_0 .net "BCDNum1", 0 3, v0x55a4032cfde0_0;  1 drivers
v0x55a4032cf850_0 .net "BCDNum2", 0 3, v0x55a4032cff10_0;  1 drivers
v0x55a4032cf8f0_0 .net "BCDRes", 0 7, L_0x55a4032d0160;  alias, 1 drivers
v0x55a4032cf990_0 .var "binRes", 0 6;
v0x55a4032cfa70_0 .var "digit1", 0 3;
v0x55a4032cfba0_0 .var "digit2", 0 3;
v0x55a4032cfc80_0 .net "validation", 0 1, L_0x55a4032d0e90;  alias, 1 drivers
E_0x55a40329ecd0 .event edge, v0x55a4032cf000_0, v0x55a4032aeac0_0;
L_0x55a4032d0160 .concat [ 4 4 0 0], v0x55a4032cfba0_0, v0x55a4032cfa70_0;
L_0x55a4032d0e90 .concat8 [ 1 1 0 0], L_0x55a4032d0d80, L_0x55a4032d07c0;
S_0x55a4032ae8f0 .scope module, "val1" "BCDValidator" 3 16, 4 7 0, S_0x55a4032ae700;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "BCDNum"
    .port_info 1 /OUTPUT 1 "out"
L_0x55a4032d0420 .functor AND 1, L_0x55a4032d0280, L_0x55a4032d0350, C4<1>, C4<1>;
L_0x55a4032d0680 .functor AND 1, L_0x55a4032d0510, L_0x55a4032d05b0, C4<1>, C4<1>;
L_0x55a4032d07c0 .functor OR 1, L_0x55a4032d0420, L_0x55a4032d0680, C4<0>, C4<0>;
v0x55a4032aeac0_0 .net "BCDNum", 0 3, v0x55a4032cfde0_0;  alias, 1 drivers
v0x55a4032ce7a0_0 .net *"_s1", 0 0, L_0x55a4032d0280;  1 drivers
v0x55a4032ce880_0 .net *"_s10", 0 0, L_0x55a4032d0680;  1 drivers
v0x55a4032ce940_0 .net *"_s3", 0 0, L_0x55a4032d0350;  1 drivers
v0x55a4032cea20_0 .net *"_s4", 0 0, L_0x55a4032d0420;  1 drivers
v0x55a4032ceb50_0 .net *"_s7", 0 0, L_0x55a4032d0510;  1 drivers
v0x55a4032cec30_0 .net *"_s9", 0 0, L_0x55a4032d05b0;  1 drivers
v0x55a4032ced10_0 .net "out", 0 0, L_0x55a4032d07c0;  1 drivers
L_0x55a4032d0280 .part v0x55a4032cfde0_0, 3, 1;
L_0x55a4032d0350 .part v0x55a4032cfde0_0, 2, 1;
L_0x55a4032d0510 .part v0x55a4032cfde0_0, 3, 1;
L_0x55a4032d05b0 .part v0x55a4032cfde0_0, 1, 1;
S_0x55a4032cee30 .scope module, "val2" "BCDValidator" 3 17, 4 7 0, S_0x55a4032ae700;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "BCDNum"
    .port_info 1 /OUTPUT 1 "out"
L_0x55a4032d0a10 .functor AND 1, L_0x55a4032d08d0, L_0x55a4032d0970, C4<1>, C4<1>;
L_0x55a4032d0c40 .functor AND 1, L_0x55a4032d0ad0, L_0x55a4032d0b70, C4<1>, C4<1>;
L_0x55a4032d0d80 .functor OR 1, L_0x55a4032d0a10, L_0x55a4032d0c40, C4<0>, C4<0>;
v0x55a4032cf000_0 .net "BCDNum", 0 3, v0x55a4032cff10_0;  alias, 1 drivers
v0x55a4032cf100_0 .net *"_s1", 0 0, L_0x55a4032d08d0;  1 drivers
v0x55a4032cf1e0_0 .net *"_s10", 0 0, L_0x55a4032d0c40;  1 drivers
v0x55a4032cf2a0_0 .net *"_s3", 0 0, L_0x55a4032d0970;  1 drivers
v0x55a4032cf380_0 .net *"_s4", 0 0, L_0x55a4032d0a10;  1 drivers
v0x55a4032cf4b0_0 .net *"_s7", 0 0, L_0x55a4032d0ad0;  1 drivers
v0x55a4032cf590_0 .net *"_s9", 0 0, L_0x55a4032d0b70;  1 drivers
v0x55a4032cf670_0 .net "out", 0 0, L_0x55a4032d0d80;  1 drivers
L_0x55a4032d08d0 .part v0x55a4032cff10_0, 3, 1;
L_0x55a4032d0970 .part v0x55a4032cff10_0, 2, 1;
L_0x55a4032d0ad0 .part v0x55a4032cff10_0, 3, 1;
L_0x55a4032d0b70 .part v0x55a4032cff10_0, 1, 1;
    .scope S_0x55a4032ae700;
T_1 ;
    %wait E_0x55a40329ecd0;
    %load/vec4 v0x55a4032cf790_0;
    %pad/u 7;
    %load/vec4 v0x55a4032cf850_0;
    %pad/u 7;
    %mul;
    %store/vec4 v0x55a4032cf990_0, 0, 7;
    %load/vec4 v0x55a4032cf990_0;
    %pad/u 32;
    %load/vec4 v0x55a4032cf990_0;
    %pad/u 32;
    %pushi/vec4 10, 0, 32;
    %mod;
    %sub;
    %pushi/vec4 10, 0, 32;
    %div;
    %pad/u 4;
    %store/vec4 v0x55a4032cfa70_0, 0, 4;
    %load/vec4 v0x55a4032cf990_0;
    %pad/u 32;
    %pushi/vec4 10, 0, 32;
    %mod;
    %pad/u 4;
    %store/vec4 v0x55a4032cfba0_0, 0, 4;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x55a4032ae400;
T_2 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55a4032cfde0_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x55a4032cff10_0, 0, 4;
    %delay 1, 0;
    %fork TD_main.dispRes, S_0x55a4032ae580;
    %join;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x55a4032cfde0_0, 0, 4;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x55a4032cff10_0, 0, 4;
    %delay 1, 0;
    %fork TD_main.dispRes, S_0x55a4032ae580;
    %join;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x55a4032cfde0_0, 0, 4;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x55a4032cff10_0, 0, 4;
    %delay 1, 0;
    %fork TD_main.dispRes, S_0x55a4032ae580;
    %join;
    %pushi/vec4 13, 0, 4;
    %store/vec4 v0x55a4032cfde0_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x55a4032cff10_0, 0, 4;
    %delay 1, 0;
    %fork TD_main.dispRes, S_0x55a4032ae580;
    %join;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x55a4032cfde0_0, 0, 4;
    %pushi/vec4 14, 0, 4;
    %store/vec4 v0x55a4032cff10_0, 0, 4;
    %delay 1, 0;
    %fork TD_main.dispRes, S_0x55a4032ae580;
    %join;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x55a4032cfde0_0, 0, 4;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x55a4032cff10_0, 0, 4;
    %delay 1, 0;
    %fork TD_main.dispRes, S_0x55a4032ae580;
    %join;
    %vpi_call 2 36 "$finish" {0 0 0};
    %end;
    .thread T_2;
